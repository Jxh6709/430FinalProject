{
  "name": "final",
  "version": "1.0.0",
  "engines": {
    "node": "12.16.0",
    "npm": "6.13.7"
  },
  "description": "igme 430 final project",
  "main": "./server/app.js",
  "scripts": {
    "buildLoginBundle": "babel ./client/login ./client/helper --presets @babel/preset-react --out-file ./hosted/loginBundle.js",
    "buildAppBundle": "babel ./client/app ./client/helper --presets @babel/preset-react --out-file ./hosted/bundle.js",
    "watchLoginBundle": "babel --watch ./client/login ./client/helper --presets @babel/preset-react --out-file ./hosted/loginBundle.js",
    "watchAppBundle": "babel --watch ./client/app ./client/helper --presets @babel/preset-react --out-file ./hosted/bundle.js",
    "nodemon": "nodemon --watch ./server ./server/app.js",
    "build": "npm run buildBrow && npm run buildLogin",
    "watch": "npm run watchBrow && npm run watchLogin",
    "buildBrow": "browserify -t [ babelify --presets [ @babel/preset-env @babel/preset-react ] ] ./client/app/components.js ./client/app/index.js ./client/app/previewComponent.js ./client/app/sendComponents.js -o ./hosted/bundle.js",
    "watchBrow": "watchify -t [ babelify --presets [ @babel/preset-env @babel/preset-react ] ] ./client/app/components.js ./client/app/index.js ./client/app/previewComponent.js ./client/app/sendComponents.js  -o ./hosted/bundle.js",
    "buildLogin": "browserify -t [ babelify --presets [ @babel/preset-env @babel/preset-react ] ] ./client/login/client.js ./client/helper/helper.js -o ./hosted/loginBundle.js",
    "watchLogin": "watchify -t [ babelify --presets [ @babel/preset-env @babel/preset-react ] ] ./client/login/client.js ./client/helper/helper.js -o ./hosted/loginBundle.js",
    "start": "node ./server/app.js",
    "pretest": "eslint ./server --fix",
    "test": "echo \"Tests complete\""
  },
  "keywords": [
    "Node",
    "MVC",
    "Redis",
    "Login"
  ],
  "author": "Cody Van De Mark",
  "license": "Apache 2.0",
